{
    "name": "yaros/inxytestplugin",
    "description": "Test plugin for INXY Wordpress PHP developer position",
    "type": "wordpress-plugin",
    "version": "0.0.1",
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "license": "GNU General Public License v2.0",
    "authors": [
        {
            "name": "Alexandr Yarosh",
            "email": "yarosh.alexandr@gmail.com"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "TestPlugin\\": "App/"
        }
    },
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.5.2",
        "gamajo/template-loader": "^1.3",
        "ext-json": "*",
        "justinrainbow/json-schema": "^5.2",
        "wpackagist-plugin/advanced-custom-fields": "^5.8"
